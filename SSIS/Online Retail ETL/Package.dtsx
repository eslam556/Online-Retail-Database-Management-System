                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	O.Order_ID AS Order_Key,
	O.Order_date AS Order_Date_Key,
	P.Product_ID AS Product_Key,
	O.Customer_ID AS Customer_Key,
	O.Payment_ID AS Payment_Key,
	O.Coupon_ID AS Coupon_Key,
	OI.Quantity AS Order_Quantity,
	ROUND(P.Price, 2) AS Product_Unit_Price,
	ROUND((P.Price * OI.Quantity), 2) AS Order_Total_Price,
	CASE
		WHEN O.Coupon_ID IS NOT NULL THEN ROUND((C.Discount_Value), 2)
		ELSE 0
	END AS Discount_Amount,
	ROUND((OI.Quantity * P.Price), 2) - 
	CASE
		WHEN O.Coupon_ID IS NOT NULL THEN ROUND((C.Discount_Value), 2)
		ELSE 0
	END AS Final_Total_Price
FROM
	[Online Retail].dbo.Orders O
JOIN
	[Online Retail].dbo.Orders_Items OI
ON
	O.Order_ID = OI.Order_ID
JOIN
	[Online Retail].dbo.Products P
ON
	OI.Product_ID = P.Product_ID
LEFT JOIN
		[Online Retail].dbo.Coupons C
ON
	O.Coupon_ID = C.Coupon_ID</property>
